<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service class="Maba\Bundle\RestBundle\Normalizer\ViolationNormalizer"
                 id="Maba\Bundle\RestBundle\Normalizer\ViolationNormalizer">
            <tag name="paysera_normalization.autoconfigured_normalizer"/>
        </service>

        <service class="Maba\Bundle\RestBundle\Normalizer\ErrorNormalizer"
                 id="Maba\Bundle\RestBundle\Normalizer\ErrorNormalizer">
            <tag name="paysera_normalization.autoconfigured_normalizer"/>
        </service>

        <service class="Maba\Bundle\RestBundle\Normalizer\Pagination\ResultNormalizer"
                 id="Maba\Bundle\RestBundle\Normalizer\Pagination\ResultNormalizer">
            <tag name="paysera_normalization.autoconfigured_normalizer"/>
        </service>

        <service class="Maba\Bundle\RestBundle\Normalizer\Pagination\PagerDenormalizer"
                 id="Maba\Bundle\RestBundle\Normalizer\Pagination\PagerDenormalizer">
            <tag name="paysera_normalization.autoconfigured_normalizer"/>
        </service>
    </services>
</container>
